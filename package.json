  "scripts": {
    "prebuild": "rimraf dist",
    "build:bff": "nest build --path src/tsconfig.build.bff.json",
    "build:client": "node scripts/build.js",
    "build": "yarn build:bff && yarn build:client",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --path src/tsconfig.bff.json",
    "start:dev": "yarn start --watch",
    "start:debug": "yarn start --debug --watch",
    "start:prod": "node dist/bff/main",
    "start:client": "node scripts/start.js",
    "test:client": "node scripts/test.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
